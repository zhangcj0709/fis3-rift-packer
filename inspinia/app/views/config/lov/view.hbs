{{#partial "content"}}
  <div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
      <h2>值列表</h2>
      <ol class="breadcrumb">
        <li><a href="/index">首页</a></li>
        <li><a>系统配置</a></li>
        <li class="active"><strong>值列表</strong></li>
      </ol>
    </div>
    <div class="col-lg-2"></div>
  </div>

  <div id="wrapper-content" class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
      <div class="col-lg-12">
        <div class="ibox float-e-margins">
          <div class="ibox-title">
            <h5>值列表</h5>

            <div class="ibox-tools">
              <a data-action="add">
                <i class="fa fa-plus-square"></i> 新增值
              </a>
              <a class="collapse-link">
                <i class="fa fa-chevron-up"></i>
              </a>
            </div>
          </div>
          <div class="ibox-content">
            {{> config/lov/templates/search}}
            {{> config/lov/templates/table}}
          </div>
        </div>
        {{> config/lov/templates/modal}}
      </div>
    </div>
  </div>
{{/partial}}
{{> main_layout}}